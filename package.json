{
  "name": "remix-cf",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public --kv=KV --d1=DB",
    "format": "pnpm prettier --write .",
    "lint": "pnpm eslint .",
    "typecheck": "tsc",
    "tail:prod": "wrangler pages deployment tail --project-name remix-cf --environment production",
    "tail:preview": "wrangler pages deployment tail --project-name remix-cf --environment preview",
    "tail:deploy": "wrangler pages deployment tail --project-name remix-cf",
    "tunnel": "cloudflared tunnel --url http://localhost:8788",
    "drizzle:generate": "drizzle-kit generate:sqlite",
    "drizzle:up": "drizzle-kit up:sqlite",
    "drizzle:check": "drizzle-kit check:sqlite",
    "d1:migrate:list": "wrangler d1 migrations list rcf_db --local",
    "d1:migrate:apply": "wrangler d1 migrations apply rcf_db --local",
    "d1:reset": "tsx scripts/d1_reset.ts",
    "d1:push": "tsx scripts/d1_push.ts",
    "script": "tsx --env-file=.dev.vars"
  },
  "dependencies": {
    "@hubspot/api-client": "^10.0.0",
    "@nextui-org/react": "^2.1.13",
    "@remix-run/cloudflare": "^2.3.1",
    "@remix-run/cloudflare-pages": "^2.3.1",
    "@remix-run/css-bundle": "^2.3.1",
    "@remix-run/react": "^2.3.1",
    "@uidotdev/usehooks": "^2.3.1",
    "ai": "^2.2.14",
    "drizzle-orm": "^0.28.6",
    "framer-motion": "^10.16.4",
    "isbot": "^3.6.8",
    "jose": "^4.14.6",
    "langchain": "^0.0.166",
    "lucide-react": "^0.287.0",
    "openai": "^4.12.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.21.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230922.0",
    "@eslint/js": "^8.49.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@remix-run/dev": "^2.3.1",
    "@types/better-sqlite3": "^7.6.6",
    "@types/eslint": "^8.44.2",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "better-sqlite3": "^9.0.0",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.49.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^13.22.0",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "^3.3.3",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "wrangler": "^3.19.0",
    "zx": "^7.2.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
